<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Appointments</title>
  </head>
  <body>
    <table>
      <thead>
        <td>name</td>
        <td>date</td>
      </thead>
      <tbody>
        {{#each data}}
